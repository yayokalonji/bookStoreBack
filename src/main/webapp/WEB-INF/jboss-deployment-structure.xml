<?xml version='1.0' encoding='UTF-8'?>
<jboss-deployment-structure>
    <deployment>
        <exclusions>
            <module name="ch.qos.logback"/>
            <module name="org.apache.logging"/>
            <module name="org.slf4j.impl"/>
            <module name="org.springframework.boot.logging"/>
        </exclusions>
        <dependencies>
            <system export="true">
                <paths>
                    <path name="jdk/net"/>
                </paths>
            </system>
        </dependencies>
    </deployment>
</jboss-deployment-structure>